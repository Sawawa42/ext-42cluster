{
	"manifest_version": 3,
	"name": "42 Profile Page Customization",
	"version": "1.0",
	"description": "42のプロフィールページで特定の処理を実行",
	"permissions": ["scripting"],
	"host_permissions": ["https://profile.intra.42.fr/*"],
	"content_scripts": [{
		"matches": ["https://profile.intra.42.fr/*"],
		"js": ["content.js"]
	}]
}
  